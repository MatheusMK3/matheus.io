{
  "version": 2,
  "name": "new.matheus.io",
  "alias": ["new.matheus.io"],
  "builds": [
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    { "src": "^/css/(.*)", "dest": "/css/$1" },
    { "src": "^/img/(.*)", "dest": "/img/$1" },
    { "src": "^/js/(.*)", "dest": "/js/$1" },
    { "src": ".*", "dest": "index.html" }
  ]
}